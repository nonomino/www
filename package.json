{
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": "23"
	},
	"packageManager": "pnpm@10.7.1",
	"pnpm": {
		"patchedDependencies": {}
	},
	"browserslist": [
		"Chrome > 0 and last 2.5 years",
		"ChromeAndroid > 0 and last 2.5 years",
		"Edge > 0 and last 2.5 years",
		"Firefox > 0 and last 2.5 years",
		"FirefoxAndroid > 0 and last 2.5 years",
		"Safari > 0 and last 2.5 years",
		"iOS > 0 and last 2.5 years",
		"not dead"
	],
	"scripts": {
		"start": "eleventy --config=.eleventy/eleventy.config.js --serve",
		"build:config": "node src/scripts/build_conf.js",
		"dev": "pnpm run build:config && eleventy --config=.eleventy/eleventy.config.js --watch",
		"build": "pnpm run build:config && eleventy --config=.eleventy/eleventy.config.js",
		"clean": "rimraf dist/",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@11ty/eleventy": "^3.0.0"
	},
	"devDependencies": {
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1"
	}
}